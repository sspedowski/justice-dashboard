{
  "name": "justice-dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:client dev:server",
    "dev:client": "vite --host",
    "dev:server": "nodemon server/server.js",
    "build:css": "postcss client/styles.css -o client/dist/styles.css",
    "watch:css": "postcss client/styles.css -o client/dist/styles.css --watch",
    "build": "npm run build:css && vite build",
    "start": "node server/server.js",
    "check": "node check-files.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"No tests specified yet\" && exit 0",
    "extract-env": "node extract-env.js",
    "cleanup": "bash cleanup-and-structure.sh",
    "cleanup:windows": "powershell -ExecutionPolicy Bypass -File cleanup-and-structure.ps1",
    "test-pdf": "python test_pdf_processing.py",
    "update-pdf": "python update_pdf_links.py"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "Chrome >= 54",
    "Firefox >= 60",
    "Safari >= 12",
    "Edge >= 79"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.2",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^10.8.0",
    "multer": "^2.0.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "tesseract.js": "^6.0.1"
  }
}
